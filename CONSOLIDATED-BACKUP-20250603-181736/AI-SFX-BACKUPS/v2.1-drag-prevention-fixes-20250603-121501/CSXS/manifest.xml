<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest
Version="6.0"
ExtensionBundleId="com.ai.sfx.generator"
ExtensionBundleVersion="2.0.0"
ExtensionBundleName="AI SFX Generator (Bolt)"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
<ExtensionList>
  <Extension Id="com.ai.sfx.generator.main" Version="2.0.0" />
</ExtensionList>
<ExecutionEnvironment>
  <HostList>
    <Host Name="PPRO" Version="[0.0,99.9]" />
  </HostList>
  <LocaleList>
    <Locale Code="All" />
  </LocaleList>
  <RequiredRuntimeList>
    <RequiredRuntime Name="CSXS" Version="9.0" />
  </RequiredRuntimeList>
</ExecutionEnvironment>
<DispatchInfoList>
  <Extension Id="com.ai.sfx.generator.main">
    <DispatchInfo>
      <Resources>
        <MainPath>./main/index.html</MainPath>
        <CEFCommandLine>
          <Parameter>--v=0</Parameter>
          <Parameter>--enable-nodejs</Parameter>
          <Parameter>--mixed-context</Parameter>
        </CEFCommandLine>
      </Resources>
      <Lifecycle>
        <AutoVisible>true</AutoVisible>
      </Lifecycle>
      <UI>
        <Type>Panel</Type>
        <Menu>AI SFX Generator (Bolt)</Menu>
        <Geometry>
          <Size>
            <Width>400</Width>
            <Height>300</Height>
          </Size>
        </Geometry>
        <Icons>
          <Icon Type="Normal">./src/assets/dark-icon.png</Icon>
          <Icon Type="DarkNormal">./src/assets/light-icon.png</Icon>
          <Icon Type="RollOver">./src/assets/dark-icon.png</Icon>
          <Icon Type="DarkRollOver">./src/assets/light-icon.png</Icon>
        </Icons>
      </UI>
    </DispatchInfo>
  </Extension>
</DispatchInfoList>
</ExtensionManifest>